cmake_minimum_required(VERSION 3.10)

project(@MYLIB_CSHARP_LIB@ CSharp)

FILE(GLOB SOURCE_FILES src/main/csharp/mae/maecsharp/*.cs)

ADD_LIBRARY(@MYLIB_CSHARP_LIB@ SHARED ${SOURCE_FILES})

set_property(TARGET @MYLIB_CSHARP_LIB@ PROPERTY WIN32_EXECUTABLE FALSE)
set_property(TARGET @MYLIB_CSHARP_LIB@ PROPERTY VS_CONFIGURATION_TYPE ClassLibrary)

install(TARGETS @MYLIB_CSHARP_LIB@ DESTINATION "@CMAKE_CURRENT_BINARY_DIR@")
