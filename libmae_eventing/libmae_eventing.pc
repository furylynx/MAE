prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
Name: libmae_eventing
Description: Eventing library of the Movement Analysis Engine (MAE) for analyzing movement using depth sensors and Labanotation.
URL: https://github.com/furylynx/MAE
Version: 0.9.0
Requires: libmae >= 0.9.0, libxml++-2.6 >= 2.6, libboost-system >= 1.5.3
Libs: -L${libdir} -lmae_eventing
Cflags: -I${includedir}
